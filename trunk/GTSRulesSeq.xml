<?xml version="1.0" ?>
<GTSRules>
	<action name="ActorSendSignal">
		<rule>actorSendSignal</rule>
	</action>
	
	<action name="AcceptEventAction">
		<exclusive-dependencies>
			<rule>acceptEventAction-AndIgnore</rule>
			<action>AcceptEventAction-WithActivity-HasBehavior</action>
			<action>AcceptEventAction-WithActivity-SameState</action>
			<action>AcceptEventAction-WithActivity-Main</action>
			<action>AcceptEventAction-WithActivity-Main-Mealy</action>
			<action>AcceptEventAction-WithActivity-Main-Mealy-SameState</action>
			<rule>acceptEventAction-WithoutActivity</rule>
			<rule>acceptEventAction-WithoutActivity-SameState</rule>
		</exclusive-dependencies>
	</action>

	<action name="AcceptEventAction-AndIgnore">
		<rule>acceptEventAction-AndIgnore</rule>
		<rule>acceptEventAction-AndIgnore-TwoTriggers</rule>
		<rule>acceptEventAction-AndIgnore-ThreeTriggers</rule>
	</action>


	<action name="AcceptEventAction-WithActivity-HasBehavior">
		<rule>acceptEventAction-WithActivity-HasBehavior</rule>
		<looping-dependency>
			<exclusive-dependencies>
				<rule>acceptEventAction-WithActivity-ExecutableEdges</rule>
				<rule>acceptEventAction-WithActivity-ExecutableEdges-Main</rule>
			</exclusive-dependencies>
		</looping-dependency>
	</action>

	<action name="AcceptEventAction-WithActivity-Main">
		<rule>acceptEventAction-WithActivity-Main</rule>
		<looping-dependency>
			<rule>acceptEventAction-WithActivity-ExecutableEdges</rule>
		</looping-dependency>
	</action>

	<action name="AcceptEventAction-WithActivity-Main-Mealy">
		<rule>acceptEventAction-WithActivity-Main-Mealy</rule>
		<looping-dependency>
			<rule>acceptEventAction-WithActivity-ExecutableEdges-Mealy</rule>
		</looping-dependency>
	</action>

	<action name="AcceptEventAction-WithActivity-Main-Mealy-SameState">
		<rule>acceptEventAction-WithActivity-Main-Mealy-SameState</rule>
		<looping-dependency>
			<rule>acceptEventAction-WithActivity-ExecutableEdges-Mealy</rule>
		</looping-dependency>
	</action>


	<action name="AcceptEventAction-WithActivity-SameState">
		<rule>acceptEventAction-WithActivity-SameState</rule>
		<looping-dependency>
			<rule>acceptEventAction-WithActivity-ExecutableEdges</rule>
		</looping-dependency>
	</action>

	<action name="SendSignalAction">
		<rule>sendSignalAction</rule>
	</action>
	
	<action name="ReadStructuralFeatureAction">
		<exclusive-dependencies>
			<rule>readStructuralFeatureAction-TwoInts</rule>
			<rule>readStructuralFeatureAction-SingleInt</rule>
		</exclusive-dependencies>
	</action>

	<action name="ReadSelfAction">
		<rule>readSelfAction</rule>
	</action>
	
	<action name="AddStructuralFeatureValueAction">
		<rule>addStructuralFeatureValueAction</rule>
	</action>
	<action name="CreateObjectAction">
		<exclusive-dependencies>
			<action>CreateObjectAction-AddStateMachineBehavior</action>
			<action>CreateObjectAction-Main</action></exclusive-dependencies>
		
		
	</action>
	<action name="CreateObjectAction-Main">
		<rule>createObjectAction-Main</rule></action>
	<action name="CreateObjectAction-AddStateMachineBehavior">
		<rule>createObjectAction-Main</rule><rule>createObjectAction-AddStateMachineBehavior</rule>
		</action>
	<action name="CallOperationAction">
		<rule>actorCallOperation</rule>
	</action>
	<action name="AcceptCallAction">
		<exclusive-dependencies>
			<action>AcceptCallAction-WithActivity-HasBehavior</action>
			<action>AcceptCallAction-WithActivity-Main</action>
		</exclusive-dependencies></action>
	<action name="AcceptCallAction-WithActivity-HasBehavior">
		<rule>acceptCallAction-WithActivity-HasBehavior</rule>
		<looping-dependency>
			<rule>acceptCallAction-WithActivity-ExecutableEdges</rule>
		</looping-dependency>
	</action>
	<action name="AcceptCallAction-WithActivity-Main">
		<rule>acceptCallAction-WithActivity-Main</rule>
		<looping-dependency>
			<rule>acceptCallAction-WithActivity-ExecutableEdges</rule>
		</looping-dependency>
	</action>
	<action name="ReplyAction">
		<rule>replyAction</rule></action>
	<action name="CreateLinkAction">
		<rule>createLinkAction-Initialise</rule>
		<looping-dependency><rule>createLinkAction-TraverseN</rule></looping-dependency><rule>createLinkAction-Traverse1</rule>
		<rule>createLinkAction-Main</rule>
	</action>

</GTSRules>